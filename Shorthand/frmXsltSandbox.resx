  <data name="txtSourceXSL.Text" xml:space="preserve">
    <value>&lt;?xml version="1.0" encoding="UTF-8"?&gt;
&lt;xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" &gt;
  &lt;xsl:output method="html" version="1.0" encoding="UTF-8" indent="yes"/&gt;
  &lt;xsl:key name="classes" match="row" use="ClassLabel"/&gt;
  &lt;xsl:template match="rows"&gt;

    &lt;html&gt;
      &lt;head&gt;        
        &lt;style type="text/css"&gt;&lt;/style&gt;
      &lt;/head&gt;

      &lt;body&gt;
        &lt;div class="md-box" style="min-height:200px"&gt;
          &lt;div id="termName"&gt;&lt;h3&gt;2015 Term 3&lt;/h3&gt;&lt;/div&gt;

          &lt;div&gt;
            &lt;table&gt;

              &lt;xsl:for-each select="//row[generate-id(.)=generate-id(key('classes', ClassLabel)[1])]"&gt;
                &lt;xsl:sort select="ClassLabel"/&gt;
                &lt;xsl:for-each select="key('classes', ClassLabel)"&gt;
                  &lt;xsl:sort select="SlotName"/&gt;

                  &lt;xsl:if test="position() = 1"&gt;
                    
                      &lt;tr class="header"&gt;
                        &lt;td class="className"&gt;&lt;xsl:value-of select="ClassLabel"/&gt;&lt;/td&gt;
                        &lt;td&gt;Monday&lt;/td&gt;
                        &lt;td&gt;Tuesday&lt;/td&gt;
                        &lt;td&gt;Wednesday&lt;/td&gt;
                        &lt;td&gt;Thursday&lt;/td&gt;
                        &lt;td&gt;Friday&lt;/td&gt;
                      &lt;/tr&gt;
                    
                  &lt;/xsl:if&gt;
                  
                  &lt;tr class="row"&gt;
                    &lt;td class="slotName"&gt;&lt;xsl:value-of select="SlotName"/&gt;&lt;/td&gt;                    
                    
                    &lt;td&gt;
                      &lt;xsl:call-template name="build_anchor"&gt;
                        &lt;xsl:with-param name="day" select="Mon_" /&gt;
                      &lt;/xsl:call-template&gt;      
                    &lt;/td&gt;                    
                    &lt;td&gt;
                      &lt;xsl:call-template name="build_anchor"&gt;
                        &lt;xsl:with-param name="day" select="Tue_" /&gt;
                      &lt;/xsl:call-template&gt;                        
                    &lt;/td&gt;
                    &lt;td&gt;
                      &lt;xsl:call-template name="build_anchor"&gt;
                        &lt;xsl:with-param name="day" select="Wed_" /&gt;
                      &lt;/xsl:call-template&gt;                       
                    &lt;/td&gt;
                    &lt;td&gt;
                      &lt;xsl:call-template name="build_anchor"&gt;
                        &lt;xsl:with-param name="day" select="Thu_" /&gt;
                      &lt;/xsl:call-template&gt;                       
                    &lt;/td&gt;
                    &lt;td&gt;
                      &lt;xsl:call-template name="build_anchor"&gt;
                        &lt;xsl:with-param name="day" select="Fri_" /&gt;
                      &lt;/xsl:call-template&gt;                       
                    &lt;/td&gt;                    
                  &lt;/tr&gt;
                  
                &lt;/xsl:for-each&gt;

                &lt;tr class="spacer"&gt;
                  &lt;td colspan="6"&gt;&lt;/td&gt;
                &lt;/tr&gt;		
                
              &lt;/xsl:for-each&gt;

            &lt;/table&gt;
          &lt;/div&gt;
        &lt;/div&gt;
      &lt;/body&gt;
    &lt;/html&gt;

  &lt;/xsl:template&gt;
  
  &lt;xsl:template name = "build_anchor" &gt;
    &lt;xsl:param name = "day" /&gt;
    &lt;xsl:variable name="instructorId" select="substring-before($day, ' : ')" /&gt;
    &lt;a&gt;                        
      &lt;xsl:attribute name="href"&gt;ISCH://&lt;xsl:value-of select="$instructorId"/&gt;.sch&lt;/xsl:attribute&gt;
      &lt;xsl:value-of select="$day"/&gt;
    &lt;/a&gt;
  &lt;/xsl:template&gt;
  
  &lt;xsl:template name="output-tokens"&gt;
      &lt;xsl:param name="list" /&gt; 
      &lt;xsl:variable name="newlist" select="concat(normalize-space($list), ':')" /&gt; 
      &lt;xsl:variable name="first" select="substring-before($newlist, ':')" /&gt; 
      &lt;xsl:variable name="remaining" select="substring-after($newlist, ':')" /&gt; 
      &lt;id&gt;
        &lt;xsl:value-of select="$first" /&gt; 
      &lt;/id&gt;
      &lt;xsl:if test="$remaining"&gt;
        &lt;xsl:call-template name="output-tokens"&gt;
          &lt;xsl:with-param name="list" select="$remaining" /&gt; 
        &lt;/xsl:call-template&gt;
      &lt;/xsl:if&gt;
  &lt;/xsl:template&gt;
  
&lt;/xsl:stylesheet&gt;
</value>
